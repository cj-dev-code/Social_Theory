############################
# CORE APP CONFIG (REQUIRED)
############################
# --- Secrets ---
# Cohere API key for reranking retrieved documents
COHERE_API_KEY=

# Discord bot token for running the feminism bot
DISCORD_BOT_TOKEN=


# --- Cohere model (can leave default) ---

# Rerank model; leave as default unless you explicitly change it in code
COHERE_RERANK_MODEL=rerank-english-v3.0


# --- Ollama (local LLM server) ---

# Name of the model served by Ollama (e.g. qwen2.5:7b-instruct or feminism_bot:latest)
OLLAMA_MODEL_NAME=qwen2.5:7b-instruct

# Base URL of your Ollama server
OLLAMA_BASE_URL=http://127.0.0.1:11434


# --- Qdrant (vector DB) ---

# Filesystem path where your Qdrant index lives
# (e.g., where you stored the experiment_9 collection)
QDRANT_PATH=

# Name of the collection containing the feminism corpus chunks
# default in config.py is experiment_9_1024_128_bge-large-en-v1.5_3_docs_take_2
QDRANT_COLLECTION=experiment_9_1024_128_bge-large-en-v1.5_3_docs_take_2



###################################
# EVALUATION / TRAINING (OPTIONAL)
# Only needed for the perplexity / DAPT scripts
###################################

# --- Hugging Face cache (for 4-bit Qwen etc.) ---
HF_CACHE=

# Base HF model used for evaluation
BASE_MODEL_ID=Qwen/Qwen2.5-7B-Instruct

# Path to your adapted Unsloth 4-bit snapshot
ADAPTED_MODEL_PATH=